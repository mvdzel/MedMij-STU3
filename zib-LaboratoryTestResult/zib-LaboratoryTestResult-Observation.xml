                </valueSetReference>
            </binding>
            <mapping>
                <identity value="hcim-laboratorytestresultrortransfer-v1.2.2-2015EN"/>
                <map value="NL-CM:13.1.4"/>
            </mapping>
            <mapping>
                <identity value="hcim-laboratorytestresultrortransfer-v1.2.2-2015EN"/>
                <map value="NL-CM:13.1.8"/>
